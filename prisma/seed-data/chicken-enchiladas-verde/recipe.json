  "steps": [
    {
      "stepNumber": 1,
      "instruction": "Preheat oven to 375\u00b0F (190\u00b0C). Lightly grease a 9x13 inch baking dish.",
      "isOptional": false
    },
    {
      "stepNumber": 2,
      "instruction": "Roast tomatillos, jalape\u00f1os, onion, and garlic under broiler for 8-10 minutes until charred.",
      "isOptional": false
    },
    {
      "stepNumber": 3,
      "instruction": "Transfer roasted vegetables to a blender. Add cilantro, chicken broth, sour cream, salt, and cumin. Blend until smooth.",
      "isOptional": false
    },
    {
      "stepNumber": 4,
      "instruction": "Warm tortillas in microwave for 30 seconds to make them pliable.",
      "isOptional": false
    },
    {
      "stepNumber": 5,
      "instruction": "Dip each tortilla in the verde sauce to coat lightly.",
      "isOptional": false
    },
    {
      "stepNumber": 6,
      "instruction": "Fill each tortilla with shredded chicken and a sprinkle of cheese. Roll tightly and place seam-side down in baking dish.",
      "isOptional": false
    },
    {
      "stepNumber": 7,
      "instruction": "Pour remaining verde sauce over enchiladas and top with remaining cheese.",
      "isOptional": false
    },
    {
      "stepNumber": 8,
      "instruction": "Bake for 25-30 minutes until cheese is melted and bubbly.",
      "isOptional": false
    },
    {
      "stepNumber": 9,
      "instruction": "Let stand 5 minutes before serving. Garnish with cilantro, sour cream, and sliced avocado.",
      "isOptional": false
    }
  ],
